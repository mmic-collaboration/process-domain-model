{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Lab Test Result",
    "properties": {
        "lab test result id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "primaryKey": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "lab test id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false,
            "alternateKey": false
        },
        "deviation id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false,
            "alternateKey": false,
            "examples": [
                10123
            ]
        },
        "recipe id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "lab test result outcome": {
            "type": "string",
            "isActivated": true,
            "autoincrement": {
                "step_num": 1
            },
            "tPrecision": 9,
            "pattern": "^[A-Za-z_][A-Za-z0-9_]*$",
            "symbols": [
                "Y"
            ],
            "compositePrimaryKey": false,
            "compositeUniqueKey": false,
            "enum": [
                "PASS",
                "FAIL"
            ]
        },
        "lab test result timestamp": {
            "type": "string",
            "isActivated": true,
            "autoincrement": {
                "step_num": 1
            },
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        }
    },
    "isActivated": true,
    "additionalProperties": false,
    "required": [
        "lab test result id",
        "lab test id",
        "deviation id",
        "lab test result outcome"
    ]
}