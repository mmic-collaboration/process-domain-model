{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Unit Process",
    "properties": {
        "unit process id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "primaryKey": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "job id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false,
            "alternateKey": false
        },
        "product batch id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "station id": {
            "type": "string",
            "isActivated": true,
            "alternateKey": false,
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "work master id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "unit process code": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "unit process start date-time": {
            "type": "string",
            "description": "This is the version of the recipe.  In SCADA this is a single integer that increments with each revision.",
            "isActivated": true,
            "autoincrement": {
                "step_num": 1
            },
            "tPrecision": 9,
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "unit process end date-time": {
            "type": "string",
            "description": "This is the version of the recipe.  In SCADA this is a single integer that increments with each revision.",
            "isActivated": true,
            "autoincrement": {
                "step_num": 1
            },
            "tPrecision": 9,
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        }
    },
    "isActivated": true,
    "additionalProperties": false,
    "required": [
        "unit process id",
        "job id",
        "product batch id",
        "work master id",
        "unit process start date-time"
    ]
}