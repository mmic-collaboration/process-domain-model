            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "product batch work order id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "product batch product id": {
            "type": "integer",
