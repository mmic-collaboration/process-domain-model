{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "batch release condition",
    "properties": {
        "batch release condition id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "primaryKey": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false,
            "examples": [
                4567890
            ]
        },
        "product batch id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false,
            "alternateKey": false
        },
        "batch release condition description": {
            "$id": "batch release condition d",
            "type": "string",
            "isActivated": true,
            "autoincrement": {
                "step_num": 1
            },
            "tPrecision": 9,
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "batch release condition outcome": {
            "type": "string",
            "isActivated": true,
            "autoincrement": {
                "step_num": 1
            },
            "tPrecision": 9,
            "pattern": "^[A-Za-z_][A-Za-z0-9_]*$",
            "symbols": [
                "Y"
            ],
            "compositePrimaryKey": false,
            "compositeUniqueKey": false,
            "enum": [
                "PASS",
                "FAIL",
                "INCONCLUSIVE"
            ],
            "examples": [
                "PASS"
            ]
        },
        "release condition rule id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false,
            "alternateKey": false
        }
    },
    "isActivated": true,
    "additionalProperties": false,
    "required": [
        "batch release condition id",
        "product batch id",
        "batch release condition description",
        "batch release condition outcome",
        "release condition rule id"
    ]
}