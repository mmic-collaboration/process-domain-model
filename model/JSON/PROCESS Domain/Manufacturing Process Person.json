{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Manufacturing Process Person",
    "properties": {
        "manufacturing process person id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "primaryKey": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "manufacturing process person name": {
            "type": "string",
            "description": "This is the version of the recipe.  In SCADA this is a single integer that increments with each revision.",
            "isActivated": true,
            "autoincrement": {
                "step_num": 1
            },
            "tPrecision": 9,
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "manufacturing process person role": {
            "type": "string",
            "isActivated": true,
            "autoincrement": {
                "step_num": 1
            },
            "tPrecision": 9,
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        }
    },
    "isActivated": true,
    "additionalProperties": false,
    "required": [
        "manufacturing process person id"
    ]
}