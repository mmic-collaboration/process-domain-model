{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Process Event Type",
    "properties": {
        "process event type id": {
            "type": "integer",
            "isActivated": true,
            "tPrecision": 9,
            "index": true,
            "primaryKey": true,
            "mode": "int32",
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "process event type name": {
            "type": "string",
            "isActivated": true,
            "autoincrement": {
                "step_num": 1
            },
            "tPrecision": 9,
            "pattern": "^[A-Za-z_][A-Za-z0-9_]*$",
            "symbols": [
                "Y"
            ],
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        },
        "process event type description": {
            "type": "string",
            "isActivated": true,
            "autoincrement": {
                "step_num": 1
            },
            "tPrecision": 9,
            "compositePrimaryKey": false,
            "compositeUniqueKey": false
        }
    },
    "isActivated": true,
    "additionalProperties": false,
    "required": [
        "process event type id",
        "process event type name"
    ]
}